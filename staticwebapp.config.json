{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "statusCode": 404,
      "body": "<h1>Page Not Found</h1><p>The page you are looking for does not exist.</p>"
    }
  }
}
